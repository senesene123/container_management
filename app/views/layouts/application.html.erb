<!DOCTYPE html>
<html>
  <head>
    <title>在庫照会</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery.tablesorter/2.31.0/js/jquery.tablesorter.min.js"></script>
  </head>

  <body>
    <div class="navbar navbar-success bg-success">
      <div class="navbar-brand">コンテナ在庫照会</div>
      <div class="navbar-collapse collapse navbar-responsive-collapse">
        <ul class="nav navbar-nav">
          <%= render 'shared/topnav_link' %>
        </ul>
        <%= image_tag 'Logotouka.png', class: "navbar-brand pull-right sized" %>
      </div>
    </div>
    <% if content_for?(:title) %><h4 class="page_title"><%= yield :title %></h4><% end %>
    <%= yield %>
  </body>
</html>
